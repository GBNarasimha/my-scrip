    <script>
        // Tab switching functionality
        document.querySelectorAll('.tab').forEach(tab => {
            tab.addEventListener('click', function() {
                document.querySelectorAll('.tab').forEach(t => t.classList.remove('active'));
                this.classList.add('active');
                console.log('Switched to:', this.dataset.tab);
            });
        });

        // Simulate live price updates
        function updatePrices() {
            const pairs = {
                eurusd: { bid: 1.09500 + Math.random() * 0.001, ask: 0.00003 },
                gbpusd: { bid: 1.27400 + Math.random() * 0.001, ask: 0.00003 },
                usdjpy: { bid: 143.800 + Math.random() * 0.2, ask: 0.03 },
                gold: { bid: 1940.50 + Math.random() * 3, ask: 0.5 }
            };
            
            const activeTab = document.querySelector('.tab.active').dataset.tab;
            const currentPair = pairs[activeTab];
            
            document.querySelectorAll('.text-right .text-xs.font-medium').forEach(el => {
                if (el.textContent.includes('Bid:')) {
                    el.innerHTML = `Bid: <span class="text-green-500">${currentPair.bid.toFixed(5)}</span>`;
                }
                if (el.textContent.includes('Ask:')) {
                    el.innerHTML = `Ask: <span class="text-red-500">${(currentPair.bid + currentPair.ask).toFixed(5)}</span>`;
                }
            });
        }
        setInterval(updatePrices, 1000);

        // Simulate new signal notification
        setInterval(() => {
            const signals = [
                {
                    type: "BUY",
                    symbol: "EUR/USD",
                    entry: (1.09500 + Math.random() * 0.0005).toFixed(5),
                    confidence: Math.floor(85 + Math.random() * 15),
                    strategy: ["Quantum Wave", "EMA Breakout", "Liquidity Grab"][Math.floor(Math.random() * 3)]
                },
                {
                    type: "SELL",
                    symbol: "USD/JPY",
                    entry: (143.800 + Math.random() * 0.1).toFixed(3),
                    confidence: Math.floor(80 + Math.random() * 15),
                    strategy: ["EMA Rejection", "Trend Exhaustion", "Fibonacci Pin"][Math.floor(Math.random() * 3)]
                },
                {
                    type: "BUY",
                    symbol: "XAU/USD",
                    entry: (1940.50 + Math.random() * 2).toFixed(2),
                    confidence: Math.floor(75 + Math.random() * 20),
                    strategy: ["Support Bounce", "Order Block", "Institutional Level"][Math.floor(Math.random() * 3)]
                }
            ];
            
            const randomSignal = signals[Math.floor(Math.random() * signals.length)];
            const signalsContainer = document.querySelector('.divide-y');
            
            const newSignal = document.createElement('div');
            newSignal.className = `signal-card ${randomSignal.type === 'BUY' ? 'buy-signal' : 'sell-signal'} p-4 hover:bg-gray-50/50 cursor-pointer glow`;
            newSignal.innerHTML = `
                <div class="flex justify-between items-start">
                    <div>
                        <div class="flex items-center mb-1">
                            <span class="${randomSignal.type === 'BUY' ? 'bg-green-500' : 'bg-red-500'} text-white text-xs font-bold px-2.5 py-0.5 rounded-full mr-2">${randomSignal.type}</span>
                            <span class="text-xs bg-blue-100 text-blue-800 px-2 py-0.5 rounded">${randomSignal.confidence}% Confidence</span>
                        </div>
                        <div class="mt-2">
                            <p class="text-sm font-medium">Entry: <span class="text-gray-900 font-bold">${randomSignal.entry}</span></p>
                            <div class="flex space-x-4 mt-1">
                                <p class="text-xs font-medium">SL: <span class="text-red-500">${(randomSignal.type === 'BUY' ? (parseFloat(randomSignal.entry) - 0.0015).toFixed(randomSignal.symbol === 'XAU/USD' ? 2 : (randomSignal.symbol === 'USD/JPY' ? 3 : 5)) : (parseFloat(randomSignal.entry) + 0.0015).toFixed(randomSignal.symbol === 'XAU/USD' ? 2 : (randomSignal.symbol === 'USD/JPY' ? 3 : 5)))}</span></p>
                                <p class="text-xs font-medium">TP1: <span class="text-green-500">${(randomSignal.type === 'BUY' ? (parseFloat(randomSignal.entry) + 0.0010).toFixed(randomSignal.symbol === 'XAU/USD' ? 2 : (randomSignal.symbol === 'USD/JPY' ? 3 : 5)) : (parseFloat(randomSignal.entry) - 0.0010).toFixed(randomSignal.symbol === 'XAU/USD' ? 2 : (randomSignal.symbol === 'USD/JPY' ? 3 : 5)))}</span></p>
                                <p class="text-xs font-medium">TP2: <span class="text-green-700">${(randomSignal.type === 'BUY' ? (parseFloat(randomSignal.entry) + 0.0025).toFixed(randomSignal.symbol === 'XAU/USD' ? 2 : (randomSignal.symbol === 'USD/JPY' ? 3 : 5)) : (parseFloat(randomSignal.entry) - 0.0025).toFixed(randomSignal.symbol === 'XAU/USD' ? 2 : (randomSignal.symbol === 'USD/JPY' ? 3 : 5)))}</span></p>
                            </div>
                        </div>
                    </div>
                    <div class="text-right">
                        <div class="text-xs ${randomSignal.strategy === 'Quantum Wave' ? 'bg-blue-600' : randomSignal.strategy === 'EMA Breakout' ? 'bg-purple-600' : 'bg-amber-600'} text-white px-2 py-1 rounded mb-1">${randomSignal.strategy}</div>
                        <p class="text-xs text-gray-500">
                            <i class="fas fa-clock mr-1"></i> <span class="blink">Just Now</span>
                        </p>
                        <div class="mt-2 flex justify-end">
                            <button class="text-xs bg-green-600 hover:bg-green-700 text-white px-3 py-1 rounded-full">
                                Execute <i class="fas fa-play ml-1"></i>
                            </button>
                        </div>
                    </div>
                </div>
            `;
            
            signalsContainer.prepend(newSignal);
            
            // Flash notification
            document.title = "⚡ New Signal - Quantum Scalper PRO";
            setTimeout(() => {
                document.title = "Quantum Scalper PRO - World's Highest Profitable MQL5 Bot";
            }, 2000);
            
            // Add notification sound in a real implementation
            console.log('New signal notification:', randomSignal);
        }, 15000); // Every 15 seconds for demo

        // Make the first signal glow
        setInterval(() => {
            const firstSignal = document.querySelector('.divide-y .signal-card');
            if (firstSignal) {
                firstSignal.classList.toggle('glow');
            }
        }, 2000);
    </script>
